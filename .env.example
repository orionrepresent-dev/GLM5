DATABASE_URL=file:./db/custom.db

# Resend Email API (crie sua conta em https://resend.com)
# 1. Acesse https://resend.com e crie uma conta gratuita
# 2. Vá em API Keys e crie uma nova key
# 3. Cole a key abaixo (começa com re_)
RESEND_API_KEY=re_xxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Email remetente (use onboarding@resend.dev para testes)
# Para produção, configure seu próprio domínio no Resend
EMAIL_FROM=onboarding@resend.dev

# Email do admin para receber notificações de novos leads
ADMIN_EMAIL=seu-email@exemplo.com
